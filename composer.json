{
    "name": "dicr/yii2-exec",
    "type": "yii2-extension",
    "license": "MIT",
    "description": "Выполнение консольных команд в Yii2",
    "keywords": ["dicr", "shell", "exec", "yii2"],
    "homepage": "https://gitlab.com/dicr/yii2-exec",
    "authors": [
        {
            "name": "Igor Tarasov",
            "email": "develop@dicr.org",
            "homepage": "https://dicr.org",
            "role": "developer"
        }
    ],
    "support": {
        "source": "https://gitlab.com/dicr/yii2-exec.git"
    },
    "minimum-stability": "stable",
    "autoload": {
        "psr-4": {
            "dicr\\exec\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "dicr\\tests\\": "tests/"
        }
    },
    "require": {
        "php": "~8.0",
        "yiisoft/yii2": "~2.0"
    },
    "require-dev": {
        "roave/security-advisories": "dev-latest",
        "phpunit/phpunit": "~9.5"
    },
    "repositories": [
        {"type": "composer", "url": "https://asset-packagist.org"}
    ]
}
